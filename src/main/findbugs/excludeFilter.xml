<FindBugsFilter>
  <!-- The maven plugin framework will set these fields with reflection,
       but findbugs thinks they are never set. -->
  <Match>
    <Class name="com.odiago.maven.plugins.hbase.StartMojo"/>
    <Or>
      <Field name="mHBaseSiteFile"/>
      <Field name="mMavenProject"/>
      <Field name="mPluginDependencyArtifacts"/>
      <Field name="mHBaseConfIndex"/>
    </Or>
    <Or>
      <Bug pattern="UWF_FIELD_NOT_INITIALIZED_IN_CONSTRUCTOR"/>
      <Bug pattern="NP_UNWRITTEN_FIELD"/>
      <Bug pattern="UWF_UNWRITTEN_FIELD"/>
    </Or>
  </Match>
</FindBugsFilter>
